{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "smells"
  ],
  "rules": {
    "quotes": [2, "single"],
    "eol-last": 0,
    "no-debugger": 1,
    "no-mixed-requires": 0,
    "no-underscore-dangle": 0,
    "no-multi-spaces": 0,
    "no-trailing-spaces": 0,
    "no-extra-boolean-cast": 0,
    "no-unused-vars": 2,
    "new-cap": 0,
    "camelcase": 2,
    "smells/no-switch": 2,
    "smells/no-complex-switch-case": 2,
    "smells/no-setinterval": 2,
    "smells/no-complex-string-concat": 2
  }
}
